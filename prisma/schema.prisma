// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db/db.db"
}

model user{
  id        String    @id @unique
  name      String
  username  String    @unique
  email     String    @unique
  passwd    String 
  createdAt DateTime  
  updatedAt DateTime?
}